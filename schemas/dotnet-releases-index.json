{
  "description": "A set of major product releases with high-level information, like latest patch version.",
  "type": [
    "object",
    "null"
  ],
  "properties": {
    "releases-index": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "description": "A major version release, including the latest patch version, and whether that patch version included security updates.",
        "type": [
          "object",
          "null"
        ],
        "properties": {
          "channel-version": {
            "type": [
              "string",
              "null"
            ]
          },
          "latest-release": {
            "type": [
              "string",
              "null"
            ]
          },
          "latest-release-date": {
            "type": "string",
            "format": "date"
          },
          "security": {
            "type": "boolean"
          },
          "latest-runtime": {
            "type": [
              "string",
              "null"
            ]
          },
          "latest-sdk": {
            "type": [
              "string",
              "null"
            ]
          },
          "product": {
            "type": [
              "string",
              "null"
            ]
          },
          "support-phase": {
            "description": "The support phases of a .NET release through its lifecycle",
            "enum": [
              "preview",
              "go-live",
              "active",
              "maintenance",
              "eol"
            ]
          },
          "eol-date": {
            "type": "string",
            "format": "date"
          },
          "release-type": {
            "description": "The release support models offering different support lengths",
            "enum": [
              "lts",
              "sts"
            ]
          },
          "releases.json": {
            "type": [
              "string",
              "null"
            ]
          },
          "patch-releases-info-uri": {
            "type": [
              "string",
              "null"
            ]
          },
          "patch-releases-index-uri": {
            "type": [
              "string",
              "null"
            ],
            "default": null
          },
          "supported-os.json": {
            "type": [
              "string",
              "null"
            ],
            "default": null
          },
          "supported-os-info-uri": {
            "type": [
              "string",
              "null"
            ],
            "default": null
          },
          "os-packages-info-uri": {
            "type": [
              "string",
              "null"
            ],
            "default": null
          }
        },
        "required": [
          "channel-version",
          "latest-release",
          "latest-release-date",
          "security",
          "latest-runtime",
          "latest-sdk",
          "product",
          "support-phase",
          "eol-date",
          "release-type",
          "releases.json",
          "patch-releases-info-uri"
        ],
        "additionalProperties": false
      }
    }
  },
  "required": [
    "releases-index"
  ]
}