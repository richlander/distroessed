        "index",
        "manifest",
        "major-release",
        "patch-release",
        "content",
        "unknown"
      ]
    },
    "_links": {
      "type": [
        "object",
        "null"
      ],
      "additionalProperties": {
        "type": [
          "object",
          "null"
        ],
        "properties": {
          "href": {
            "type": [
              "string",
              "null"
            ]
          },
          "relative": {
            "type": [
              "string",
              "null"
            ]
          },
          "title": {
            "type": [
              "string",
              "null"
            ]
          },
          "type": {
            "type": [
              "string",
              "null"
            ]
          }
        },
        "required": [
          "href"
        ]
      }
    },
    "version": {
      "type": [
        "string",
        "null"
      ]
    },
    "label": {
      "type": [
        "string",
        "null"
      ]
    },
    "ga-date": {
